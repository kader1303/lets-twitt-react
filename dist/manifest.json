{
    "name": "Let's Twitt",
    "version": "1.0",
    "description": "Recuerda twittear varias veces al d√≠a.",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "alarms",
      "notifications",
      "tabs",
      "browserAction"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_icon": "/assets/tweet128.png"
    },
    "icons": {
      "16": "/assets/tweet16.png",
      "32": "/assets/tweet32.png",
      "64": "/assets/tweet64.png",
      "128": "/assets/tweet128.png"
    },    
    "action": {
      "default_title": "Let's Twitt",
      "default_popup": "popup.html",
      "default_icon": "/assets/tweet128.png"
    }
  }
  